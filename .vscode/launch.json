    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "npm: build",
            "program": "${workspaceFolder}/node_modules/homebridge/bin/homebridge",
            "args": ["-I", "-D"],
            "outFiles": [
                //"${workspaceFolder}/**/*.js"
                "${workspaceFolder}/dist/**/*.js"
            ],
            "env": {
                "WEBHOOK_URL": "https://stwh.kleinstudios.net/test/api/"
            }
        }
    ]
}